<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="300.0" prefWidth="300.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
         <children>
            <TableView fx:id="messagesTable" layoutX="50.0" layoutY="50.0" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="dateCol" editable="false" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" resizable="false" sortable="false" style="width: 100%;" text="Date" />
                  <TableColumn fx:id="messagesCol" editable="false" maxWidth="150.0" minWidth="150.0" prefWidth="150.0" resizable="false" sortable="false" style="width: 100%;" text="Messages" />
              </columns>
            </TableView>
         </children>
      </Pane>
   </children>
</GridPane>
